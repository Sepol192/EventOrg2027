@model EventOrg2027.Models.TipoEventos

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<h3></h3>
<div>
    <h3>Tipos de Eventos</h3>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Nome do Tipo de Eventos
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NomeTipoEventos)
        </dd>
        <div class="form-group">
            @if (User.IsInRole("Admin"))
            {
                <a asp-action="Edit" asp-route-id="@Model.TipoEventosId" class="btn btn-primary">Editar</a>
                <p></p>
                <p><a asp-action="Index" class="btn btn-dark">Voltar</a></p>
            }
            </div>
    </dl>
</div>

