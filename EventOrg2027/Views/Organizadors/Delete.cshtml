@model EventOrg2027.Models.Organizador

@{
    ViewData["Title"] = "Delete";
}

<h1>Delete</h1>

<h4>.</h4>
<div>
    <h3>Eliminar Organizador</h3>
    <h5>Tem a certeza que deseja eliminar "@Html.DisplayFor(model => model.NomeOrganizador)"?</h5>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Organizador ou Organização
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NomeOrganizador)
        </dd>
        <dt class="col-sm-2">
            Contacto
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Contacto)
        </dd>
        <dt class="col-sm-2">
            Data Parceria
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DataNascimento)
        </dd>
        <dt class="col-sm-2">
            EmailAddress
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.EmailAddress)
        </dd>
        <form asp-action="Delete">
            <input type="hidden" asp-for="OrganizadorId" />
            <input type="submit" value="Apagar" class="btn btn-danger" /> |
            <p></p>
            <p><a asp-action="Index" class="btn btn-dark">Voltar para a lista</a></p>
        </form>
    </dl>

    </div>
